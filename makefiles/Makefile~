SOURCES = $(LFS)/sources
BOLD    = '\033[1m'
RED     = '\e[1;31m'
GREEN   = '\e[0;32m'
ORANGE  = '\e[0;33m'
BLUE    = '\e[1;34m'
WHITE   = '\033[37m'
YELLOW  = '\e[1;33m'

OFF     = '\e[0m'
REVERSE = '\e[7m'

tab_    = '	'
nl_     = ''

define echo_message
  @echo $(BOLD)--------------------------------------------------------------------------------
  @echo -e $(BOLD)$(1) target $(BLUE)$@$(BOLD)$(WHITE)
endef
