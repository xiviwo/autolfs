cat > blfs-yes-test1 << "EOF"
#!/bin/bash

echo -n -e "

Please type something (or nothing) and press Enter ---> "

read A_STRING

if test "$A_STRING" = ""; then A_STRING="Just the Enter key was pressed"
else A_STRING="You entered '$A_STRING'"
fi

echo -e "

$A_STRING

"
EOF
chmod 755 blfs-yes-test1
